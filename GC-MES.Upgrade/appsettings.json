{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft": "Warning",
      "System": "Warning"
    },
    "Console": {
      "LogLevel": {
        "Default": "Information"
      }
    },
    "File": {
      "Path": "logs/upgrade-.log",
      "Append": true,
      "FileSizeLimitBytes": 10485760,
      "MaxRollingFiles": 10
    }
  },
  "Updater": {
    "ServerUrl": "http://update.gc-mes.com",
    "DefaultAction": "check",
    "CheckIntervalHours": 24,
    "AutoDownload": false,
    "AutoInstall": false,
    "EnableSilentUpdate": false
  }
} 